{
  "name": "console",
  "version": "1.0.3",
  "description": "The globular admin console. It also contain unit test and documentation.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --mode production && cp -r css dist && cp -r images dist && cp index.html dist && cp test.html dist && Globular deploy  -u=sa -p=adminadmin -name=console -a=omniscient.app -path=dist"
